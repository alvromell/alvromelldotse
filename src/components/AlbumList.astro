---
import type { CollectionEntry } from "astro:content";
import AlbumCard from "./AlbumCard.astro";

type Props = {
    albums: CollectionEntry<'albums'>[];
}

const { albums } = Astro.props;
---
<div class="flex flex-col gap-1 justify-items-center mb-16">
    {albums.map((album) => <AlbumCard album={album}/>)}
</div>
